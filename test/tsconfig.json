{
    "compilerOptions": {
        "strict": true,
        "sourceMap": true,
        "declarationMap": true,
        "declaration": true,
        "baseUrl": "/never",
        "outDir": "../dist-test/",
        "composite": true,
        "incremental": true,
        "tsBuildInfoFile": "../dist-test/.tsbuildinfo",
        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "target": "ES5",
        "downlevelIteration": true,
        "lib": [
            "ES6"
        ]
    },
    "references": [
        {"path": "../tsconfig.json"}
    ],
    "include": [
        "./**/*"
    ]
}